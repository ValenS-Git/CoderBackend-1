<h2 class="main-title">Lista de Productos</h2>

<div id="productList">
    {{#each products}}
    <div class="card">
        <img src="{{thumbnails}}" alt="{{title}}">
        <div class="card-body">
            <div class="card-title">
                <h3>{{title}}</h3>
                <p class="price">$ {{price}}</p>
            </div>
            <p>{{description}}</p>
        </div>
    </div>
    {{/each}}
</div>